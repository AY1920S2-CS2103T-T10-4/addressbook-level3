<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<GridPane fx:id="deadlinePane" hgap="5" prefHeight="132.0" prefWidth="510.0" vgap="5" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="80.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
    </columnConstraints>
    <Label fx:id="name" styleClass="activity_big_label" text="\$code" wrapText="true" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Label fx:id="date" styleClass="activity_small_label" text="\$code" wrapText="true" GridPane.columnIndex="0" GridPane.rowIndex="1">
        <minWidth>
            <Region fx:constant="USE_PREF_SIZE" />
        </minWidth>
    </Label>
    <Label fx:id="status" styleClass="activity_small_label" text="\$code" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <minWidth>
            <Region fx:constant="USE_PREF_SIZE" />
        </minWidth>
    </Label>
    <Label fx:id="priority" styleClass="activity_priority_label" text="\$code" wrapText="true" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="0">
        <minWidth>
            <Region fx:constant="USE_PREF_SIZE" />
        </minWidth>
    </Label>
    <Label fx:id="note" styleClass="activity_small_label" text="\$code" wrapText="true" GridPane.rowIndex="3">
        <minWidth>
            <Region fx:constant="USE_PREF_SIZE" />
        </minWidth>
    </Label>
   <Label fx:id="dateToRepeat" text="\$code" GridPane.rowIndex="2" />
   <Circle fx:id="circle" fill="DODGERBLUE" radius="15.0" stroke="BLACK" strokeType="INSIDE" translateX="44.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
   <rowConstraints>
      <RowConstraints maxHeight="36.0" minHeight="21.0" prefHeight="35.0" />
      <RowConstraints maxHeight="35.0" minHeight="0.0" prefHeight="16.0" />
      <RowConstraints maxHeight="34.0" minHeight="0.0" prefHeight="16.0" />
      <RowConstraints maxHeight="46.0" minHeight="0.0" prefHeight="20.0" />
      <RowConstraints maxHeight="37.0" minHeight="0.0" prefHeight="0.0" />
      <RowConstraints maxHeight="39.0" minHeight="0.0" prefHeight="0.0" />
      <RowConstraints maxHeight="41.0" minHeight="0.0" prefHeight="0.0" />
      <RowConstraints maxHeight="44.0" minHeight="0.0" prefHeight="0.0" />
   </rowConstraints>
</GridPane>
